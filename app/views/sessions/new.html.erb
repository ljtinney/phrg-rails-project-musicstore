<h1>Sessions#new</h1>

<h1>Please Sign In</h1>

<%= form_for(@customer, url: signin_path) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>
<br>
   <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>
<br>
   <%= f.label :username %>
  <%= f.text_field :username %><br>
<br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
<br>
  <%= f.submit "Sign In" %>
<% end %>