<h1>Customers NEW View Test</h1>

<%= form_for @customer do |f| %>
  <div>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
    <br>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
    <br>
    <%= f.label :username%><br>
    <%= f.text_field :username%><br>
    <br>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br>
    <%= f.submit %>
<% end %>
</div>